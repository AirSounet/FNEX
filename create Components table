dict.Add Key:="Accumulator: Gas", Item:="Flownex.Components."
dict.Add Key:="Fixed Volume", Item:="Flownex.Components."


If dict.Exists(FMcompName) Then
    'MsgBox FMcompName & " exists and has value " & dict(FMcompName)
    CheckComponent = dict(FMcompName)
Else
    MsgBox FMcompName & " does not exist."
End If
    
    
Set dict = Nothing

End Function
